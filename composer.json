{
    "name": "runopencode/exchange-rate-intesa-rs",
    "description": "Plugin for 'runopencode/exchange-rate' library for fetching exchange rates from Banca Intesa Serbia.",
    "keywords": [
        "exchange rate", "banca intesa serbia", "banca intesa"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Stojan Trajkovic",
            "email": "stole54@gmail.com",
            "homepage": "http://www.runopencode.com",
            "role": "Project lead"
        },
        {
            "name": "RunOpenCode members",
            "email": "support@runopencode.com",
            "homepage": "http://www.runopencode.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RunOpenCode\\ExchangeRate\\NationalBankOfSerbia\\": "src/RunOpenCode/ExchangeRate/BancaIntesaSerbia/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RunOpenCode\\ExchangeRate\\NationalBankOfSerbia\\Tests\\": "test/"
        }
    },
    "require": {
        "php": ">=5.5",
        "psr/log": "1.*",
        "guzzlehttp/guzzle": "~6.1",
        "symfony/dom-crawler": "~2.8|~3.0",
        "symfony/css-selector": "~2.8|~3.0",
        "runopencode/exchange-rate": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "mockery/mockery": "~0.9"
    },
    "suggest": {
        "runopencode/exchange-rate-bundle": "Exchange rate Symfony bundle."
    }
}
